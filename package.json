{
  "name": "query-doctor-local-sync",
  "version": "0.0.1",
  "description": "A tool to generate SQL commands for syncing PostgreSQL database structures",
  "license": "UNLICENSED",
  "author": "",
  "bin": {
    "query-doctor-local-sync": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc --noEmit && nest build -b swc && npm run sentry:sourcemaps",
    "dev": "TS_NODE_BASEURL=./dist nest start -b swc --watch --exec 'node -r tsconfig-paths/register ./dist/main.js'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "antlr-setup.sh",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/main.js",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org qd-4q --project api ./dist && sentry-cli sourcemaps upload --org qd-4q --project api ./dist"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/schedule": "^6.0.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "pg-structure": "^7.15.3",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.29",
    "@types/express": "^5.0.2",
    "@types/node": "22.15.29",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.10",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "1.2.1"
  }
}
